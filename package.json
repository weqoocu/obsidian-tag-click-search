  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/weqoocu/obsidian-tag-click-search.git"
  },
  "bugs": {
    "url": "https://github.com/weqoocu/obsidian-tag-click-search/issues"
  },
  "homepage": "https://github.com/weqoocu/obsidian-tag-click-search#readme"
